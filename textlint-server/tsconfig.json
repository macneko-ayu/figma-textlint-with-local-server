{
  "compilerOptions": {
    "target": "ES2020",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}

